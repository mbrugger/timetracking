# encoding: UTF-8
<% if !working_day.custom_description.nil? && working_day.custom_description.size > 0 %>
  <%= working_day.custom_description %>
<% elsif !working_day.leave_day.nil? %>
  <%= working_day.leave_day.localized_leave_day_type %>
<% end %>
<% working_day.time_entries.each do |time_entry| %>
  <%= time_entry.startTime.to_formatted_s(:pretty_time)%>-<%= time_entry.stopTime.to_formatted_s(:pretty_time) unless time_entry.stopTime.nil? %>
<% end %>
